//You are given with a number "N", find its cube.

const readline = require('readline');
const inp = readline.createInterface({
  input: process.stdin
});
const userInput = [];
inp.on("line", (data) => {
	userInput.push(data);
});
inp.on("close", () => {
	console.log(cube(userInput));
});

function cube(Data){
  var a = Data;
  var c = 0;
 for(var i=0;i<cube.length;i++)
  {
   a[i]=parseInt(a[i]);
    c=a*a*a;
  }
  return c;
}
    
    
      
